    "body-parser": "^1.19.0",
    "buffer": "^6.0.1",
    "core-js": "^3.6.5",
    "cypress": "^4.12.1",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "serve": "^11.3.2",
    "signature_pad": "^3.0.0-beta.4",
    "ts-jest": "^26.4.3",
    "ts-node": "^9.0.0",
    "util": "^0.12.3",
    "vue": "^2.6.12",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
